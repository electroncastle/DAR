net: "train_val_fast.prototxt"
test_iter: 10
test_interval: 500

# last -9 at 18000

# -10/800 -8/1000 -6
base_lr: 1e-8

#base_lr:  0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
display: 20
max_iter: 80000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "snapshot"
iter_size: 1
